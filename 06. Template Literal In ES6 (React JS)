import "./App.css";

function App() {
  const esFeature = "Template Literal using variable interpolation";

  const sum = (x, y) => x + y;
  const x = 5;
  const y = 10;
  return (
    <div className="App">
      {`Hello World! ${esFeature} `}
      <br />
      {`The Sum of x:${x} and y:${y} is ${sum(x, y)}`}
      <br />
      {`The Name of This Channel is "Developers Diary". `}
    </div>
  );
}

export default App;
