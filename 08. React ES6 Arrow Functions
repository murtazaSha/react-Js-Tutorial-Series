import "./App.css";

function App() {
  const sum = function (x, y) {
    return x + y;
  };

  const sum1 = (x, y) => x + y;

  const exp = (x) => `expression ! ${x} `;

  const exp2 = (x) => {
    const a = x.split("-")[0];
    return a;
  };

  const exp3 = (x) => x.split("-")[0];

  return (
    <div className="App">
      {sum(2, 7)}
      <br />
      {sum1(3, 5)}
      <br />
      {exp("Hello world")}
      <br />
      {exp2("1982-12-19")}
      <br />
      {exp3("1983-12-19")}
    </div>
  );
}

export default App;
